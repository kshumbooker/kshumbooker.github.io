<script nonce="ZAhZa+UNasyjmpRs4IzK9CiWyOro3CnDqlzNCDA4qiY=">
    Handlebars.registerHelper('iff', function (a, operator, b, opts) {
        var bool = false;
        switch (operator) {
            case '&&':
                bool = a && b;
                break;
            default:
                throw "Unknown operator " + operator;
        }

        if (bool) {
            return opts.fn(this);
        } else {
            return opts.inverse(this);
        }
    });
    var miniTrolleyMobileTemplate = $('#mini-trolley-mobile-template').html();

    var templateCompile = Handlebars.compile(miniTrolleyMobileTemplate);

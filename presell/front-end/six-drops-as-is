<script nonce="ZAhZa+UNasyjmpRs4IzK9CiWyOro3CnDqlzNCDA4qiY=">
    $(document).ready(function () {

        $('.remove-link').click(function (e) {

            e.preventDefault();
            var customerNumber = $(this).data("item");

            var loadingText = '<div style="display:flex;justify-content: center;width: 100%;"><img src="/images/ajax-loader.gif"> <span style="margin-left:10px">Removing account please wait...</span></div>';
            $('#switch-account .modal-footer').html(loadingText);

            $.ajax({
                type: "POST",
                url: '/api/sitecore/Accounts/RemoveLink',
                data: { customerNumber: customerNumber }
            })
                .done(function (data) {
                    if (data.status === "success") {
                        window.location.assign(window.location.href);
                    }
                })
                .fail(function (xhr, textStatus, errorThrown) {
                    console.log(xhr.responseText);
                    window.location.reload(true);
                });
        });

        $('.switch-user').click(function () {

            var loadingText = '<div style="display:flex;justify-content: center;width: 100%;"><img src="/images/ajax-loader.gif"> <span style="margin-left:10px">Switching accounts please wait...</span></div>';

            $('#switch-account .modal-footer').html(loadingText);

            var customerNumber = $(this).attr("data-item");

            $.ajax({
                type: "POST",
                url: '/api/sitecore/Accounts/SwitchAccount',
                data: { customerNumber: customerNumber }
            })
            .done(function (data) {
                if (data.RedirectUrl !== null && data.RedirectUrl !== '' && data.RedirectUrl !== undefined) {
                    window.location.assign(data.RedirectUrl);
                }
                else { window.location.assign('/home'); }
            })
            .fail(function (xhr, textStatus, errorThrown) {
                console.log(xhr.responseText);
                window.location.reload();
            });
        });

    });
</script>


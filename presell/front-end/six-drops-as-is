    $(document).ready(function () {
        document.getElementById('barcode-picker-starter-button').addEventListener("click", function (evt) { startBarcodePicker();; });
        document.getElementById('scanner-stop-button').addEventListener("click", function (evt) { stopBarcodePicker(); });
        document.getElementById('scanner-clear-button').addEventListener("click", function (evt) { clearSearchResults();});

    });


    function getCookieValue(cookieName) {
        var value = document.cookie.match('(^|[^;]+)\\s*' + cookieName + '\\s*=\\s*([^;]+)');
        return value ? value.pop() : '';
    }

</script>
<script nonce="ZAhZa+UNasyjmpRs4IzK9CiWyOro3CnDqlzNCDA4qiY=">

    if (typeof trolley_mode == 'undefined') {
        trolley_mode = 1;
    }


    function ActivateTrolley(trolleyType, refreshPage, redirectUrl) {
        var payload = {
            trolleyType: trolleyType
        }

        $.ajax({
            url: '/api/sitecore/Trolley/ActivateTrolley',
            type: 'POST',
            dataType: "json",
            contentType: 'application/json',
            data: JSON.stringify(payload)
        }).done(function (data) {
            if (refreshPage === undefined || refreshPage !== false) {
                location.reload();
            }
            if (redirectUrl) {
                window.location.href = redirectUrl;
            }
        });
    }

